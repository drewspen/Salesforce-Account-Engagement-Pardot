const injectScript = require('injectScript');
const setInWindow = require('setInWindow');
const callInWindow = require('callInWindow');
const log = require('logToConsole');

// Set Pardot configuration variables
setInWindow('piAId', data.pardotAccountId);
setInWindow('piCId', data.pardotCampaignId);
const piHostname = data.piHostname; 
setInWindow('piHostname', data.piHostname );

// Define the Pardot initialization function
const pardotInit = function() {
  // Create and inject the Pardot tracking script
  // const protocol = require('getUrl')('protocol');
  const scriptUrl = 'https://' +  piHostname + '/pd.js';
  
  injectScript(scriptUrl, function() {
    log('Pardot script loaded successfully');
    data.gtmOnSuccess();
  }, function() {
    log('Pardot script failed to load');
    data.gtmOnFailure();
  });
};

// Set the initialization function in window
setInWindow('pardotInit', pardotInit);

// Execute the initialization
callInWindow('pardotInit');


